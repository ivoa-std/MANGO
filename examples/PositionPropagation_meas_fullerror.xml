<VODML xmlns:dm-mapping="http://www.ivoa.net/xml/mivot">
    <REPORT status="OK">hand-made mapping</REPORT>

    <MODEL name="meas" url="https://www.ivoa.net/xml/Meas/20200908/Meas-v1.0.vo-dml.xml" />
    <MODEL name="coords" url="https://www.ivoa.net/xml/STC/20200908/Coords-v1.0.vo-dml.xml" />
    <MODEL name="mango" />
    <MODEL name="ivoa" url="https://www.ivoa.net/xml/VODML/IVOA-v1.vo-dml.xml" />

    <GLOBALS>
        <INSTANCE dmid="SpaceFrame_ICRS" dmtype="coords:SpaceSys">
            <INSTANCE dmrole="coords:PhysicalCoordSys.frame" dmtype="coords:SpaceFrame">
                <INSTANCE dmrole="coords:SpaceFrame.refPosition" dmtype="coords:StdRefLocation">
                    <ATTRIBUTE dmrole="coords:StdRefLocation.position" dmtype="ivoa:string" value="NoSet" />
                </INSTANCE>
                <ATTRIBUTE dmrole="coords:SpaceFrame.spaceRefFrame" dmtype="ivoa:string" value="ICRS" />
                <ATTRIBUTE dmrole="coords:SpaceFrame.equinox" dmtype="coords:Epoch" value="2015.0" />
            </INSTANCE>
        </INSTANCE>
    </GLOBALS>

    <TEMPLATE>
        <!-- One meas:Measure per row, full error -->
        <INSTANCE dmtype="mango:PositionPropagation">
            <ATTRIBUTE dmrole="mango:PositionPropagation.longitude" dmtype="ivoa:RealQuantity" ref="ra" unit="deg"/>
            <ATTRIBUTE dmrole="mango:PositionPropagation.latitude" dmtype="ivoa:RealQuantity" ref="dec" unit="deg"/>
            <ATTRIBUTE dmrole="mango:PositionPropagation.pmLongitude" dmtype="ivoa:RealQuantity" ref="pmra" unit="mas/year"/>
            <ATTRIBUTE dmrole="mango:PositionPropagation.pmLatitude" dmtype="ivoa:RealQuantity" ref="pmdec" unit="mas/year"/>
            <ATTRIBUTE dmrole="mango:PositionPropagation.radialVelocity" dmtype="ivoa:RealQuantity" ref="rv" unit="km/dec"/>
            <ATTRIBUTE dmrole="mango:PositionPropagation.parallax" dmtype="ivoa:RealQuantity" ref="parallax" unit="mas"/>
            <REFERENCE dmref="SpaceFrame_ICRS" dmrole="coords:coordSys" />
            <INSTANCE dmrole="meas:Measure.error" dmtype="mango:PositionPropagationError">
                <!-- Elliptical error for position -->
                <INSTANCE dmrole="mango:PositionPropagationError.positionError" dmtype="mango:Ellipse">
                    <ATTRIBUTE dmrole="mango:MultiParamError.confidence" dmtype="ivoa:integer" value="1"/>
                    <ATTRIBUTE dmrole="mango:MultiParamError.unit" dmtype="ivoa:unit" value="deg"/>
                    <ATTRIBUTE dmrole="mango:Ellipse.semiMajorAxis" dmtype="ivoa:real" ref="maj_axis"/>
                    <ATTRIBUTE dmrole="mango:Ellipse.semiMinorAxis" dmtype="ivoa:real" ref="maj_axis"/>
                    <ATTRIBUTE dmrole="mango:Ellipse.angle" dmtype="ivoa:RealQuantity" ref="angle" unit="deg"/>
                </INSTANCE>
                <!-- Covariance error for proper motion -->
                <INSTANCE dmrole="mango:PositionPropagationError.pmError" dmtype="mango:CorrMatrix2x2">
                    <ATTRIBUTE dmrole="mango:MultiParamError.confidence" dmtype="ivoa:integer" value="1"/>
                    <ATTRIBUTE dmrole="mango:MultiParamError.unit" dmtype="ivoa:unit" value="mas/year"/>
                    <ATTRIBUTE dmrole="mango:CorrMatrix2x2.sigma1" dmtype="ivoa:real" ref="pmRA_err" />
                    <ATTRIBUTE dmrole="mango:CorrMatrix2x2.sigma2" dmtype="ivoa:real" ref="pmRA_err" />
                    <ATTRIBUTE dmrole="mango:CorrMatrix2x2.correlation" dmtype="ivoa:real" ref="pmRADEC_err" />
                </INSTANCE>
                <!-- Symetrical error for proper radial velocity -->
                <INSTANCE dmrole="mango:PositionPropagationError.radialVelocityError" dmtype="meas:Symetrical">
                    <ATTRIBUTE dmrole="meas:Symetrical.val" dmtype="ivoa:realQuantity" ref="rv_err" unit="km/sec"/>
                </INSTANCE>
                <!-- Symetrical error for proper parallax -->
                <INSTANCE dmrole="mango:PositionPropagationError.parallaxError" dmtype="">
                    <ATTRIBUTE dmrole="meas:Symetrical.val" dmtype="ivoa:realQuantity" ref="parallax_err" unit="mas"/>
                </INSTANCE>
                <!-- Correlation error between PM and POS -->
                <INSTANCE dmrole="mango:PositionPropagationError.pmPosError" dmtype="mango:CorrelatedError_2Dwith2D">
                    <ATTRIBUTE dmrole="mango:CorrelatedError_2Dwith2D.correlation_1_1" dmtype="ivoa:real" ref="pmRA_RA_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_2Dwith2D.correlation_2_2" dmtype="ivoa:real" ref="pmDEC_DEC_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_2Dwith2D.correlation_1_2" dmtype="ivoa:real" ref="pmRA_DEC_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_2Dwith2D.correlation_2_1" dmtype="ivoa:real" ref="pmDEC_RA_err"/>
                </INSTANCE>

                <!-- Correlation error between parallax and POS -->
                <INSTANCE dmrole="mango:PositionPropagationError.parallaxPosError" dmtype="mango:CorrelatedError_1Dwith2D">
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_1" dmtype="ivoa:real" ref="parallax_RA_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_2" dmtype="ivoa:real" ref="parallax_DEC_err"/>
                </INSTANCE>

                <!-- Correlation error between parallax and PM -->
                <INSTANCE dmrole="mango:PositionPropagationError.parallaxPmError" dmtype="mango:CorrelatedError_1Dwith2D">
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_1" dmtype="ivoa:real" ref="parallax_pmRA_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_2" dmtype="ivoa:real" ref="parallax_pmDEC_err"/>
                </INSTANCE>

                <!-- Correlation error between radial velocity and POS -->
                <INSTANCE dmrole="mango:PositionPropagationError.parallaxPosError" dmtype="mango:CorrelatedError_1Dwith2D">
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_1" dmtype="ivoa:real" ref="rv_RA_err"/>
                    <ATTRIBUTE dmrole="mango:CorrelatedError_1Dwith2D.correlation_1_2" dmtype="ivoa:real" ref="rv_DEC_err"/>
                </INSTANCE>

            </INSTANCE>
        </INSTANCE>
    </TEMPLATE>
</VODML>

